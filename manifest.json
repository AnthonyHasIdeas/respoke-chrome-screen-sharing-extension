{
  "manifest_version": 2,
  "name": "Respoke Screenshare",
  "description": "This extension simplifies the usage of the respoke library for initiating a screensharing session.",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://d2wy8f7a9ursnm.cloudfront.net https://www.respoke.io https://ajax.googleapis.com; object-src 'self'",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
